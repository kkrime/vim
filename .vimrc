" leader key 
let mapleader = " "

" Pluings 
source ~/.vim/vundle.vim
source ~/.vim/commentry.vim
source ~/.vim/coc.vim
source ~/.vim/go-vim.vim
source ~/.vim/autoindent.vim
source ~/.vim/vim-airline.vim
source ~/.vim/auto-pairs.vim
source ~/.vim/vim-surround.vim


" disbale .swp file creation/check
set noswapfile

" set tab
set tabstop=4
set shiftwidth=4
set expandtab 
set autoindent
set smartindent

" show linenumber
set nu
